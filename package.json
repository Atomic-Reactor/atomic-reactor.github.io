{
  "name": "reactium",
  "version": "2.1.0",
  "description": "A framework for creating React + Redux apps using the domain driven design (DDD) paradigm.",
  "main": "index.js",
  "homepage": "https://atomic-reactor.github.io",
  "scripts": {
    "start": "node build/.core/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "manifest": "node ./createManifest.js",
    "build": "npm install --production-only && npm-run-all -s manifest build:*",
    "build:gulp": "cross-env NODE_ENV=production gulp",
    "build:babel-core": "cross-env NODE_ENV=production babel .core --out-dir build/.core",
    "build:babel-src": "cross-env NODE_ENV=production babel src --out-dir build/src",
    "local": "npm install && npm run manifest && npm run local-fe-start",
    "local-fe-start": "npm-run-all -p local-fe:*",
    "local-fe:gulp": "cross-env SSR_MODE=off NODE_ENV=development gulp",
    "local-fe:babel-node": "cross-env SSR_MODE=off NODE_ENV=development nodemon ./.core/index.js --exec babel-node",
    "local-ssr": "npm install && npm run manifest && npm run local-ssr-start",
    "local-ssr-start": "npm-run-all -p local-ssr:*",
    "local-ssr:gulp": "cross-env SSR_MODE=on NODE_ENV=development gulp",
    "local-ssr:babel-node": "cross-env SSR_MODE=on NODE_ENV=development nodemon ./.core/index.js --exec babel-node",
    "clean": "gulp clean",
    "docker": "npm-run-all -s clean docker:*",
    "docker:build": "docker image build -t atomicreactor/reactium:$npm_package_version .",
    "docker:push": "docker push atomicreactor/reactium:$npm_package_version",
    "deploy": "gh-pages -d docs"
  },
  "keywords": [
    "react",
    "redux",
    "framework",
    "javascript",
    "ddd"
  ],
  "author": "Cam Tullos <cam@tullos.ninja> (http://cam.tullos.ninja) and John Dillick <john@dillick.us>",
  "license": "MIT",
  "engines": {
    "node": "9.2.1",
    "npm": ">=2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Atomic-Reactor/Reactium.git"
  },
  "browser": {
    "parse/node": false
  },
  "dependencies": {
    "ajv": "^6.5.0",
    "axios": "^0.19.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "cross-env": "^6.0.3",
    "directory-tree": "^2.0.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.1.0",
    "gh-pages": "^2.1.1",
    "http-auth": "^3.2.3",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "object-path": "^0.11.4",
    "parse": "^2.10.0",
    "querystring-browser": "^1.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.1.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-local-persist": "^0.1.0",
    "redux-super-thunk": "0.0.8",
    "serialize-javascript": "^2.1.1",
    "slug": "^1.1.0",
    "underscore": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "2.26.7",
    "chalk": "^3.0.0",
    "del": "^5.1.0",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-if": "^3.0.0",
    "gulp-install": "^1.1.0",
    "gulp-less": "^4.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "handlebars": "^4.0.11",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "readdir-recursive": "0.0.4",
    "run-sequence": "^2.2.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.8.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "yargs": "^15.0.2"
  },
  "nodemonConfig": {
    "watch": [
      "src/manifest.js"
    ],
    "ignore": [
      "src/app/*"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "stage-2",
      [
        "env",
        {
          "useBuiltIns": true,
          "debug": false,
          "targets": {
            "browsers": [
              ">= 1%"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "cwd": "packagejson",
          "alias": {
            "manifest": "./src/manifest",
            "appdir": "./src/app",
            "components": "./src/app/components",
            "reactium-core": "./.core",
            "dependencies": "./.core/dependencies"
          }
        }
      ]
    ]
  }
}
